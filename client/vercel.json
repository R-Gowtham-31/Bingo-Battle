{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "computer.html",
      "use": "@vercel/static"
    },
    {
      "src": "online.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static/public/main.js"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/computer",
      "dest": "/computer.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
 "rewrites": [{ "source": "/(.*)", "destination": "/" }] 

}
