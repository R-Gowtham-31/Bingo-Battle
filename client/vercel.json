{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static"},
    { "src": "computer.html", "use": "@vercel/static"},
    { "src": "online.html", "use": "@vercel/static"}
  ],
  "routes": [
    { "src": "/computer", "dest": "/computer.html" },
    { "src": "/online", "dest": "/online.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
